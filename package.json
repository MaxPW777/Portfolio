{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "main-portfolio",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run start:dev",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm build && npm start",
    "start:backend": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaxPW777/Portfolio.git"
  },
  "author": "MaxPW777",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MaxPW777/Portfolio/issues"
  },
  "homepage": "https://github.com/MaxPW777/Portfolio#readme",
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.36.1",
    "globals": "^15.9.0",
    "typescript-eslint": "^8.7.0"
  }
}
